@{
    ViewBag.Title = "Germany Customers";
}

<h2>Germany Customers</h2>

<table class="table">
    <thead>
        <tr>
            <th>Customer ID</th>
            <th>Company Name</th>
            <!-- Add more columns if needed -->
        </tr>
    </thead>
    <tbody>
        @foreach (var customer in Model)
        {
            <tr>
                <td>@customer.CustomerID</td>
                <td>@customer.CompanyName</td>
                <!-- Add more columns if needed -->
            </tr>
        }
    </tbody>
</table>
